@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Графики</title>
    @Styles.Render("~/Content/newcss")
    @Scripts.Render("~/bundles/chartscripts")
    <script src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.2.2/Chart.bundle.min.js"></script>
    <script>
        buildLineChart(@Html.Raw(Json.Encode(@ViewBag.CommonResponseTimes)), 
            @Html.Raw(Json.Encode(@ViewBag.DelayedResponseTimes)), 
            @Html.Raw(Json.Encode(@ViewBag.ContentIds)),
            "Response Times", 
            "Linecanvas");
    </script>
</head>
<body>
    <div id="wrapper">
        <div class="container">
            <div class="box">
                <div class="graphcontainer">
                    <div style="text-align: center">
                        <canvas id="Linecanvas"></canvas>
                    </div>
                </div>
            </div>
        </div>
    </div>
</body>
</html>